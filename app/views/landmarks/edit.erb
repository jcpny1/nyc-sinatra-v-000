<h1>Landmark</h1>

<form method="post" action="/landmarks/<%= @landmark.id %>">
  <input id="hidden" type="hidden" name="_method" value="patch">
  <p>Name: <input type="text" id="name" name="landmark[name]" value=<%= @landmark.name %>></p>
  <p>Year: <input type="text" id="year_completed" name="landmark[year_completed]" value=<%= @landmark.year_completed %>></p>
  <br>
  <p><input type="submit" value="Edit Landmark"></p>
</form>
